<div class="collapse" id="ankle_mmt_score">
  <div class="card card-body">
    <h3>足関節</h3>
    <div class="d-flex justify-content-center">
      <div class="ankle_mmt_head part-head">
        <dd class="left-top"></dd>
        <dd>背屈</dd>
        <dd>底屈</dd>
        <dd>内返し</dd>
        <dd>外返し</dd>
      </div>
      <div class="right_ankle_mmt_score mmt-score">
        <dd class="direction-head">右</dd>
        <% mmt_scale.right_ankle_scale_score.keys.each  do |attr_name| %>
          <dd class="<%= attr_name %>_score"><%= mmt_scale.send("#{attr_name}_i18n") %></dd>
        <% end %>
      </div>
      <div class="left_ankle_mmt_score mmt-score">
        <dd class="direction-head">左</dd>
        <% mmt_scale.left_ankle_scale_score.keys.each  do |attr_name| %>
          <dd class="<%= attr_name %>_score"><%= mmt_scale.send("#{attr_name}_i18n") %></dd>
        <% end %>
      </div>
    </div>
  </div>
</div>
