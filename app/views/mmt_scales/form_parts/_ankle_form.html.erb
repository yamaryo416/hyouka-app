<div class="collapse ankle_mmt_form" id="ankle_mmt_form">
  <div class="card card-body">
    <h3>足関節</h3>
    <div class="d-flex justify-content-center">
      <div class="ankle_mmt_head part-head">
        <dd class="left-top"></dd>
        <dd>背屈</dd>
        <dd>底屈</dd>
        <dd>内返し</dd>
        <dd>外返し</dd>
      </div>
      <div class="right_ankle_mmt_form mmt-form">
        <dd class="direction-head">右</dd>
        <% mmt_scale.right_ankle_scale_attributes.each  do |attr_name| %>
          <div>
            <%= f.select attr_name, MmtScale.human_select_options(attr_name) %>
          </div>
        <% end %>
      </div>
      <div class="left_ankle_mmt_form mmt-form">
        <dd class="direction-head">左</dd>
        <% mmt_scale.left_ankle_scale_attributes.each  do |attr_name| %>
          <div>
            <%= f.select attr_name, MmtScale.human_select_options(attr_name) %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
